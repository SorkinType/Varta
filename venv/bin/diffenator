#!/Users/Viviana/Documents/06_GOOGLE/GF_Fonts/Varta/venv/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'fontdiffenator==0.9.1','console_scripts','diffenator'
__requires__ = 'fontdiffenator==0.9.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('fontdiffenator==0.9.1', 'console_scripts', 'diffenator')()
    )
